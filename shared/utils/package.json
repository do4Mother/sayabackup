{
  "name": "@sayabackup/utils",
  "version": "1.0.0",
  "description": "Shared utilities for sayabackup monorepo",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit",
    "deploy": "npm run build"
  },
  "keywords": [
    "utils",
    "shared"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "tsup": "^8.0.0",
    "typescript": "~5.9.3"
  },
  "dependencies": {
    "crypto-js": "^4.2.0"
  }
}